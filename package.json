    "build": "tailwindcss -o dist/tailwind.css"
  },
  "peerDependencies": {
    "tailwindcss": "^3.4.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "concurrently": "^5.3.0",
    "live-server": "^1.2.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  },
  "dependencies": {
    "mini-svg-data-uri": "^1.2.3"
  }
}