{
  "name": "tlwind-forms",
  "version": "0.0.1",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "license": "MIT",
  "repository": "https://github.com/paiameya/tailwindcss-forms",
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
